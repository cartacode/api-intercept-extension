{
  "name": "Scrape video url from tidal",
  "description": "Extension that scrapes tidal vidoe url",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "webRequest",
    "*://*.google.com/",
    "*://developer.chrome.com/*",
    "*://*.video.tidal.com/*"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons/get_started16.png",
      "32": "images/icons/get_started32.png",
      "48": "images/icons/get_started48.png",
      "128": "images/icons/get_started128.png"
    }
  },
  "manifest_version": 2
}
