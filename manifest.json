{
  "name": "One Sream Player",
  "description": "Chrome Extension for One stream player",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons/get_started16.png",
      "32": "images/icons/get_started32.png",
      "48": "images/icons/get_started48.png",
      "128": "images/icons/get_started128.png"
    }
  },
  "manifest_version": 2
}
